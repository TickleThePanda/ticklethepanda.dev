{
  "name": "ticklethepanda.co.uk",
  "version": "1.0.0",
  "description": "",
  "": "",
  "scripts": {
    "less": "lessc _less/style.less public/default.css",
    "minify-css": "cleancss -o public/default.css public/default.css",
    "compress-jpeg": "find ./public/images -name \"*.jpg\" -type f -exec jpegtran -copy none -optimize -outfile {} {} \\;",
    "compress-gif": "find ./public/images -name \"*.gif\" -type f -exec gifsicle --colors 256 -O3 --careful -o {} {} \\;",
    "compress-images": "npm run compress-jpeg && npm run compress-gif",
    "start-app": "node app.js",
    "start": "npm run less && npm run minify-css && npm run compress-images && npm run start-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticklethepanda/ticklethepanda.co.uk.git"
  },
  "author": "",
  "dependencies": {
    "clean-css": "^3.4.12",
    "express": "^4.14.0",
    "express-minify-html": "^0.6.0",
    "htmling": "0.0.7",
    "less": "^2.7.1",
    "request": "^2.78.0"
  }
}
